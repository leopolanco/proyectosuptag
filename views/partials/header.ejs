                  <li class="nav-item">
                     <a class="nav-link" href="/registro">Registrarse</a>
                  </li>
             <% } else { %>
                  <li class="nav-item pr-3">
                     <a class="nav-link" href="#"><%=currentUser.username%></a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link" href="/logout">Salir</a>
                  </li>
               <% } %>   
                  
                  </div>
            </ul>
            
             
         </nav>

<html>
   <head>
      <title>Proyectos</title>
      <link rel="stylesheet"  type="text/css" href="/css/bootstrap.min.css">
            <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">
      <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/1.6.1/css/buttons.dataTables.min.css">
      <style type="text/css">

         .btn-warning{
            padding: 10px 40px 10px 40px;
         }
         footer, nav{
            background: #1C2331;
            fixed;
         }
         footer a {
            color: grey;
         }
         hr.style13 {
            border-top: 1px solid #ffa400;
            text-align: center;
         }
         footer{
            margin-top:auto; 
         }
         .buscar {
            text-transform:capitalize;
         }
          #fichaMostrar, input, textarea {
            text-transform: uppercase;
         }
         
         form {
            display: block;
            margin-top: 0em;
            margin-block-end: 0em;
         }
      </style>
      
      
        </head>
      
   <body>
      <div class="shadow-lg">
         <nav class="navbar navbar-expand-lg mr-auto navbar-dark bg-dark ">
            <a class="navbar-brand" href="/inicio"><img class="pl-1 " src="/images/iconuptag.jpg"></a>

               <ul class="navbar-nav mr-auto">
                  <li class="nav-item">
                     <a class="nav-link" href="/inicio">Inicio</span></a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link" href="/archivar">Archivar</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link" href="#">Ayuda</a>
                  </li>
               </ul> 
                     <a class="text-light nav-link" href="/logout">Salir</a>

                 
         </nav>
         </div>   
<div class="container mt-5">
<table id="tablaProy" class="display" style="width:100%">
        <thead>
            <tr>
                <th>Titulo</th>
                <th>Pnf</th>
                <th>Comunidad</th>
                <th>Trayecto</th>
                <th>Lapso Academico</th>
                <th></th>
            </tr>
        </thead>
        
        <tbody>
            <% proys.forEach(function(proy){ %>
            <tr>
                <td><%=proy.tituloProyecto%></td>
                <td><%=proy.pnf%></td>
                <td><%=proy.comunidad%></td>
                <td><%=proy.trayecto%></td>
                <td><%=proy.lapsoAcademico%></td>
                <td><a href="/index/<%=proy._id %>">... Ver m√°s</a></td>
            </tr>
            <% }) %>
           
           
           
    </table>
</div>

<footer id = "footer" class=" text-white-50 pt-3">

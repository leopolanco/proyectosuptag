<script src="/js/main.js"></script> 

<script>
$(document).ready(function() {
    $('#tablaProy').DataTable( {
        dom: 'Bfrtip',
        buttons: [
            'excelHtml5',
        ]
    } );
} );

</script>
<script>
$(document).ready(function() {
            
		$(".titulo").shorten({
            
	        "showChars" : 50,
	        "moreText"	: "más",
	        "lessText"	: "menos",
        });
        $(".paginate_button").click(function(){
	        $(".titulo").shorten({
	        "showChars" : 50,
	        "moreText"	: "más",
	        "lessText"	: "menos",
            });
        });
        $("sorting_asc").click(function(){
	        $(".titulo").shorten({
	        "showChars" : 50,
	        "moreText"	: "más",
	        "lessText"	: "menos",
            });
        });
        $("sorting_desc").click(function(){
	        $(".titulo").shorten({
	        "showChars" : 50,
	        "moreText"	: "más",
	        "lessText"	: "menos",
            });
        });
    });

</script>


</body>
</html>